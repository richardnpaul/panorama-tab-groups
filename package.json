  "description": "Tab Groups Viewer",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:lint": "npm run lint",
    "lint": "npm-run-all lint:*",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint --ext=.js,.json .",
    "lint:webext": "web-ext -s src lint",
    "format": "npm-run-all format:*",
    "format:prettier": "prettier --write .",
    "format:eslint": "eslint --ext=.js,.json --fix .",
    "precommit": "pre-commit run",
    "build": "web-ext build -s src",
    "watch": "web-ext run -s src",
    "start": "web-ext run -s src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richardnpaul/panorama-tab-groups.git"
