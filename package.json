  },
  "homepage": "https://github.com/richardnpaul/panorama-tab-groups#readme",
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "web-ext": "^9.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
